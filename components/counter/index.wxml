<!--components/counter/index.wxml-->
<l-counter
        l-counter-class="all-background input-size"
        l-symbol-class="all-background font-color"
        l-disable-class="all-background"
        l-class="container"
        count="{{count}}"
        min="{{min}}"
        max="{{max}}"
        bind:linout="onOverStep"
/>
